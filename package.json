{
  "name": "dinky.js",
  "version": "1.1.0",
  "description": "JavaScript bindings for Derpibooru API",
  "main": "lib/Dinky",
  "types": "lib/Dinky.d.ts",
  "repository": "octet-stream/dinky",
  "author": "Nick K. <io@octetstream.me>",
  "license": "MIT",
  "engines": {
    "node": ">= 10 < 11 || >= 12 < 13 || >= 14"
  },
  "keywords": [
    "api-bindings",
    "derpibooru",
    "api-client"
  ],
  "scripts": {
    "test": "ava",
    "report": "nyc npm test",
    "report:html": "nyc npm test && nyc report --reporter=html",
    "report:json": "nyc npm test && nyc report --reporter=json",
    "ci": "npm run report:json && node_modules/.bin/codecov",
    "eslint": "eslint readme.md lib/**/*.js test/**/*.js",
    "staged": "lint-staged"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "5.0.0",
    "ava": "3.15.0",
    "codecov": "3.8.1",
    "eslint": "7.18.0",
    "eslint-plugin-ava": "11.0.0",
    "eslint-plugin-markdown": "1.0.2",
    "fetch-mock": "9.11.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "sinon": "9.2.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/node-fetch": "2.5.8",
    "camelcase-keys": "6.2.2",
    "node-fetch": "2.6.1"
  }
}

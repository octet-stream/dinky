{
  "name": "dinky.js",
  "version": "2.0.0-beta.1",
  "description": "JavaScript bindings for Philomena JSON API. Supports sites such as Derpibooru and Furbooru.",
  "type": "module",
  "types": "./@type/dinky.d.ts",
  "exports": "./lib/dinky.js",
  "repository": "octet-stream/dinky",
  "author": "Nick K. <io@octetstream.me>",
  "license": "MIT",
  "engines": {
    "node": ">= 12.20"
  },
  "keywords": [
    "api-bindings",
    "derpibooru",
    "furbooru",
    "api-client",
    "philomena"
  ],
  "scripts": {
    "build": "tsc && tsc --project tsconfig.d.ts.json",
    "cleanup": "npx rimraf @type lib",
    "test": "ava --fail-fast",
    "coverage": "c8 npm test",
    "ci": "c8 npm test && c8 report --reporter=json",
    "eslint": "eslint src/**/*.ts --ext .ts",
    "staged": "lint-staged",
    "prepare": "npm run cleanup && npm run build",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "5.0.0",
    "@types/node": "16.4.13",
    "@types/sinon": "10.0.2",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "ava": "3.15.0",
    "c8": "7.8.0",
    "codecov": "3.8.3",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-ava": "12.0.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "fetch-mock": "9.11.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "pinst": "2.1.6",
    "sinon": "11.1.2",
    "ts-node": "10.2.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "camelcase-keys": "7.0.0",
    "cross-fetch": "3.1.4"
  }
}
